ls.js",[],[],"/home/everson/projetos/react/cadastro-usuario/frontend/src/main/app.jsx",["67"],[],"import \"./app.css\"\nimport \"bootstrap/dist/css/bootstrap.min.css\"\nimport \"font-awesome/css/font-awesome.min.css\"\n\nimport React from \"react\"\n\nimport Logo from \"../components/template/logo\"\nimport Nav from \"../components/template/nav\"\nimport Footer from \"../components/template/footer\"\nimport Routes from \"./routes\"\n\nimport {BrowserRouter as Router} from \"react-router-dom\"\n\nexport default props => {\n    return (\n        <div className=\"app\">\n            <Router>\n                <Logo></Logo>\n                <Nav></Nav>\n                <Routes></Routes>\n                <Footer></Footer>\n            </Router>\n        </div>\n    )\n}\n\n\n","/home/everson/projetos/react/cadastro-usuario/frontend/src/components/template/nav.jsx",["68"],[],"/home/everson/projetos/react/cadastro-usuario/frontend/src/components/template/footer.jsx",["69"],[],"import React from \"react\";\nimport \"./footer.css\"\n\nexport default props => (\n    <footer className=\"footer\">\n        <span>\n            Desenvolvido com <i className=\"fa fa-heart text-danger\"></i> por <strong>Cod <span className=\"text-danger\">3</span>r</strong>\n        </span>\n    </footer>\n)","/home/everson/projetos/react/cadastro-usuario/frontend/src/components/template/main.jsx",["70"],[],"import React from \"react\"\nimport Header from \"./header\"\nimport \"./main.css\"\n\nexport default props => (\n    <>\n        <Header {...props}></Header>\n        <main className=\"content container-fluid\">\n            <div className=\"p-3 mt-3\">\n                {props.children}\n            </div>\n        </main>\n    </>\n)\n\n\n","/home/everson/projetos/react/cadastro-usuario/frontend/src/components/template/logo.jsx",["71"],[],"/home/everson/projetos/react/cadastro-usuario/frontend/src/components/template/header.jsx",["72"],[],"import React from \"react\";\nimport \"./header.css\"\n\nexport default props => (\n    <header className=\"header d-none d-sm-flex flex-column\">\n        <h1 className=\"mt-3\">\n            <i className={`fa fa-${props.icon}`}></i> {props.title}\n        </h1>\n        <p className=\"lead text-muted\">{props.subtitle}</p>\n    </header>\n)","/home/everson/projetos/react/cadastro-usuario/frontend/src/components/home/home.jsx",["73"],[],"import React from \"react\"\nimport Main from \"../template/main\"\n\nexport default props => (\n    <Main icon=\"home\" title=\"Inicio\" subtitle=\"Segundo Projeto do capítulo de React\">\n        <div className=\"display-4\">\n            Bem vindo!\n        </div>\n        <hr />\n        <p className=\"mb-0\">Sistema para exemplificar a construção de um cadastro desenvolvido em React!</p>\n    </Main>\n)","/home/everson/projetos/react/cadastro-usuario/frontend/src/main/routes.jsx",["74"],[],"/home/everson/projetos/react/cadastro-usuario/frontend/src/components/user/userCrud.jsx",["75"],[],"import React, {Component, useReducer} from \"react\"\nimport Main from \"../template/main\"\nimport axios from \"axios\"\n\nconst headerProps = {\n    icon: \"users\",\n    title: \"Usuários\",\n    subtitle: \"Cadastro de usuários: Incluir, Listar, Alterar e Excluir\"\n}\n\nconst baseUrl = \"http://localhost:3001/users\"\nconst initialState = {\n    user: {name: \"\", email: \"\"},\n    list: []\n}\n\nexport default class UserCrud extends Component{\n    state = { ...initialState }\n\n    componentWillMount(){\n        axios(baseUrl).then(resp => {\n            this.setState({list: resp.data})\n        })\n    }\n\n    clear(){\n        this.setState({user: initialState.user})\n    }\n\n    save(){\n        const user = this.state.user\n        const method = user.id ? \"put\" : \"post\" \n        const url = user.id ? `${baseUrl}/${user.id}` : baseUrl\n        axios[method](url, user).then(resp => {\n            const list = this.getUpdatedList(resp.data)\n            this.setState({user: initialState.user, list})\n        })\n    }\n\n    getUpdatedList(user, add=true){\n        const list = this.state.list.filter(u => u.id !== user.id)\n        if(add) list.unshift(user)\n        return list\n    }\n\n    updateField(event){\n        const user = {...this.state.user}\n        user[event.target.name] = event.target.value\n        this.setState({ user })\n    }\n\n    renderForm(){\n        return (\n            <div className=\"form\">\n                <div className=\"row\">\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label>Nome</label>\n                            <input type=\"text\" className=\"form-control\" name=\"name\" value={this.state.user.name} onChange={e => this.updateField(e)} placeholder=\"Digite o nome...\"/>\n                        </div>\n                    </div>\n\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label>Email</label>\n                            <input type=\"text\" className=\"form-control\" name=\"email\" value={this.state.user.email} onChange={e => this.updateField(e)} placeholder=\"Digite o email...\"/>\n                        </div>\n                    </div>\n                </div>\n\n                <hr />\n\n                <div className=\"row\">\n                    <div className=\"col-12 d-flex justify-content-end\">\n                        <button className=\"btn btn-primary\" onClick={e => this.save(e)}>Salvar</button>\n                        <button className=\"btn btn-secondary ml-2\" onClick={e => this.clear(e)}>Cancelar</button>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n    load(user){\n        this.setState({ user })\n    }\n    \n    remove(user){\n        axios.delete(`${baseUrl}/${user.id}`).then(resp => {\n            const list = this.getUpdatedList(user, false)\n            console.log(list)\n            this.setState({ list })\n        })\n    }\n\n    renderTable(){\n        return (\n            <table className=\"table mt-4\">\n                <thead>\n                    <tr>\n                        <th>ID</th>\n                        <th>Nome</th>\n                        <th>Email</th>\n                        <th>Ações</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {this.renderRows()}\n                </tbody>\n            </table>\n        )\n    }\n\n    renderRows(){\n        const trs = this.state.list.map(u => {\n            return(\n                <tr key={u.id}>\n                    <td>{u.id}</td>\n                    <td>{u.name}</td>\n                    <td>{u.email}</td>\n                    <td>\n                        <button className=\"btn btn-warning\" onClick={() => this.load(u)}>\n                            <i className=\"fa fa-pencil\"></i>\n                        </button>\n                        <button className=\"btn btn-danger ml-2\"  onClick={() => this.remove(u)}>\n                            <i className=\"fa fa-trash\"></i>\n                        </button>\n                    </td>\n                </tr>\n            )       \n        })\n\n        return trs\n    }\n\n    render(){\n        return (\n            <Main {...headerProps}>\n                {this.renderForm()}\n                {this.renderTable()}\n            </Main>\n        )\n    }\n}",["76","77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":14,"column":1,"nodeType":"84","endLine":25,"endColumn":2},{"ruleId":"82","severity":1,"message":"83","line":6,"column":1,"nodeType":"84","endLine":19,"endColumn":2},{"ruleId":"82","severity":1,"message":"83","line":4,"column":1,"nodeType":"84","endLine":10,"endColumn":2},{"ruleId":"82","severity":1,"message":"83","line":5,"column":1